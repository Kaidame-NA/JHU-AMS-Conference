---
  const { } = Astro.props;
---

<section>
  <a class = "anchor relative -top-10" id = "schedule" />
  <div class="max-w-6xl mx-auto px-4 sm:px-6">
    <div class="py-12 md:py-20">
      <div class="py-4 sm:py-6 lg:py-8">
        <div class="flex flex-wrap -mx-8">
          <div class="w-full px-8">
            <div class="mb-12 lg:mb-0 pb-12 lg:pb-0 border-b lg:border-b-0 text-center">
              <h2 class="text-4xl md:text-5xl font-bold leading-tighter tracking-tighter mb-4">
                Schedule
              </h2>
              <p class="mb-8 text-xl text-gray-600 dark:text-gray-400">
                Schedule text
              </p>
            </div>
          </div>
        </div>
      </div>

    </div>
  </div>
</section>